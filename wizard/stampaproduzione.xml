<?xml version="1.0" ?>
<openerp>
    <data>
        <record id="view_stampa_mrp_production" model="ir.ui.view">
            <field name="name">Stampe di Produzione</field>
            <field name="model">stampa.produzione</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                    <form string="Parametri di Stampa"  >
                       <!--   <field name="dadata" colspan="4" />
