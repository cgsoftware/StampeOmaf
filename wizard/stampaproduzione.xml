        <act_window id="action_view_stampa_mrp_production"
            key2="client_print_multi"
            name="Stampa di produzione"
            res_model="stampa.produzione"
            src_model="mrp.production"
            view_mode="form" target="new" view_type="form" />
       
        
    </data>
