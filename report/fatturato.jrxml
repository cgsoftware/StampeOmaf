     fiscaldoc_righe."sconti_riga" AS fiscaldoc_righe_sconti_riga,
     fiscaldoc_righe."product_id" AS fiscaldoc_righe_product_id,
     fiscaldoc_righe."totale_riga" AS fiscaldoc_righe_totale_riga,
     fiscaldoc_righe."name" AS fiscaldoc_righe_name,
     fiscaldoc_righe."discount_riga" AS fiscaldoc_righe_discount_riga,
     fiscaldoc_righe."order_line_id" AS fiscaldoc_righe_order_line_id,
     fiscaldoc_righe."codice_iva" AS fiscaldoc_righe_codice_iva,
     fiscaldoc_righe."descrizione_riga" AS fiscaldoc_righe_descrizione_riga,
     fiscaldoc_righe."move_ids" AS fiscaldoc_righe_move_ids,
     fiscaldoc_righe."product_prezzo_unitario" AS fiscaldoc_righe_product_prezzo_unitario,
     fiscaldoc_righe."prezzo_netto" AS fiscaldoc_righe_prezzo_netto,
     fiscaldoc_righe."peso_conai" AS fiscaldoc_righe_peso_conai,
     fiscaldoc_righe."cod_conai" AS fiscaldoc_righe_cod_conai,
     fiscaldoc_righe."prezzo_conai" AS fiscaldoc_righe_prezzo_conai,
     fiscaldoc_righe."product_uos_qty" AS fiscaldoc_righe_product_uos_qty,
     fiscaldoc_causalidoc."tipo_operazione" AS fiscaldoc_causalidoc_tipo_operazione,
     product_category."name" AS product_category_name
FROM
     "public"."fiscaldoc_header" fiscaldoc_header RIGHT OUTER JOIN "public"."fiscaldoc_righe" fiscaldoc_righe ON fiscaldoc_header."id" = fiscaldoc_righe."name"
     LEFT OUTER JOIN "public"."fiscaldoc_causalidoc" fiscaldoc_causalidoc ON fiscaldoc_header."tipo_doc" = fiscaldoc_causalidoc."id"
     LEFT OUTER JOIN "public"."product_product" product_product ON fiscaldoc_righe."product_id" = product_product."id"
     LEFT OUTER JOIN "public"."product_template" product_template ON product_product."product_tmpl_id" = product_template."id"
     LEFT OUTER JOIN "public"."product_category" product_category ON product_template."categ_id" = product_category."id"
WHERE
     fiscaldoc_causalidoc."tipo_operazione" = 'C'
 AND fiscaldoc_header."data_documento" >= to_date($P{dadata},'YYYY-MM-DD')
 AND fiscaldoc_header."data_documento" <= to_date($P{adata},'YYYY-MM-DD')
 AND fiscaldoc_header."tipo_doc" >= $P{tipodoc}
 AND fiscaldoc_header."tipo_doc" <= $P{atipodoc}]]>
